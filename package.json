  "jest": {
    "globals": {
      "navigator": {
        "userAgent": "node.js"
      }
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.spec\\.(ts|js)$",
    "setupFiles": [
      "<rootDir>/test/jest-pretest.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!**/*.spec.{js,ts}",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageDirectory": "<rootDir>/test/coverage-jest",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "start": "nps",
    "test": "nps test",
    "preversion": "npm test",
    "version": "standard-changelog && git add CHANGELOG.md",
    "prepare": "au build-plugin",
    "build:plugins": "NODE_OPTIONS=--max-old-space-size=4096 au build-plugin",
    "build:binding-behaviours": "au build-plugin --projects binding-behaviours",
    "build:bootstrap": "au build-plugin --projects bootstrap",
    "build:bootstrap-plus": "au build-plugin --projects bootstrap-plus",
    "build:components": "au build-plugin --projects components",
    "build:custom-attributes": "au build-plugin --projects custom-attributes",
    "build:jquery": "au build-plugin --projects jquery",
    "build:persian-value-converters": "au build-plugin --projects persian-value-converters",
    "build:services": "au build-plugin --projects services",
    "build:value-converters": "au build-plugin --projects value-converters",
    "postversion": "git push && git push --tags",
    "cypress:run": "./node_modules/.bin/cypress run",
    "cypress:open": "./node_modules/.bin/cypress open"
  },
  "main": "dist/app.bundle.js"
}
