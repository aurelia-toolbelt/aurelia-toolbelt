    "humanize-duration": "^3.12.0",
    "jalali-moment": "^3.0.5",
    "moment-timezone": "^0.5.14",
    "persianjs": "^0.3.0",
    "strman": "^2.0.0",
    "written-number": "^0.8.1"
  },
  "aurelia": {
    "usedBy": [],
    "build": {
      "resources": [
        "aurelia-toolbelt",
        "aurelia-toolbelt/index"
