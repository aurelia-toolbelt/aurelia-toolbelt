    "test-watch": "./node_modules/.bin/jest --config=jest.config.json --watch",
    "build-all": "node setup & gulp build & gulp test"
  },
  "devDependencies": {
    "@types/jest": "^21.1.6",
    "@types/moment-timezone": "^0.5.1",
    "@types/node": "^8.0.51",
