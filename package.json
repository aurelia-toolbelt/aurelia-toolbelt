{
  "name": "aurelia-toolbelt",
  "version": "0.5.20",
  "description": "Create typescript plugins for Aurelia",
  "keywords": [
    "aurelia",
    "plugin",
    "toolbelt",
    "aurelia toolbelt"
  ],
  "homepage": "https://github.com/shahabganji/aurelia-toolbelt",
  "bugs": {
    "url": "https://github.com/shahabganji/aurelia-toolbelt/issues"
  },
  "license": "MIT",
