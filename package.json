    "aurelia-pal-nodejs": "^1.0.0-beta.2.0.0",
    "aurelia-polyfills": "^1.2.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-plumber": "^1.1.0",
    "gulp-run": "^1.7.1",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "^3.2.3",
    "jest": "^21.2.1",
    "jsdom": "^11.3.0",
    "merge2": "^1.2.0",
    "require-dir": "^0.3.2",
    "run-sequence": "^2.2.0",
    "ts-jest": "^21.2.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vinyl-paths": "^2.1.0"
