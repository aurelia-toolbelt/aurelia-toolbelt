{
  "transform": {
    "^.+\\.tsx?$": "./node_modules/ts-jest/preprocessor.js"
  },
  "testRegex": ".*(\\.test|\\.spec)\\.(jsx?|tsx?)",
  "testPathIgnorePatterns": [
    "build"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json"
  ],
  "testEnvironment": "node",
  "setupFiles": [
    "<rootDir>/test/jest.setup.ts"
  ],
  "modulePaths": [
    "<rootDir>/src",
    "<rootDir>/node_modules"
  ]
}